tdm_door_latch_01
{
    qer_editorimage	models/darkmod/props/textures/tdm_door_latch_01_s
        metal

		{
			blend diffusemap
			map models/darkmod/props/textures/tdm_door_latch_01_s
				red .6
				green .6
				blue .5
		}
			{
				maskcolor				
				map makealpha(models/darkmod/props/textures/tdm_door_latch_01_mask)
			}
			{
				forceHighQuality
				blend gl_dst_alpha, gl_one
				cameraCubeMap env/currency_camera/coin
				texgen reflect
			}
		{
			blend specularmap
			map models/darkmod/props/textures/tdm_door_latch_01_s
			rgb 1
		}

	bumpmap		models/darkmod/props/textures/tdm_door_latch_01_local

}

